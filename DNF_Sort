#include<iostream>
using namespace std;

void swap(int i, int j)
{
    int temp=i;
    i=j;
    j=temp;
}

void dnsSort(int arr[], int n)
{
    int low=0;
    int mid=0;
    int high=n-1;

    while(mid<=high)
    {
        if(arr[mid==0])
        {
            swap(arr[low],arr[mid]);
            low++;mid++;
        }
        else if(arr[mid]==1)
        {
            mid++;
        }
        else
        {
            swap(arr[mid],arr[high]);
            high--;
        }
        
    }
}

int main()
{
    int n;
    cout<<"Enter size of array"<<endl;
    cin>>n;
    int arr[n];
    cout<<"Enter array elements. Make sure it has only 0 , 1 , 2"<<endl;
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    
    dnsSort(arr,n);
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    
    return 0;
}
